<config>
    <modules>
        <SSE_TranslationHints>
            <version>0.1.0</version>
        </SSE_TranslationHints>
    </modules>
    <global>
        <helpers>
            <sse_translationhints>
                <class>SSE_TranslationHints_Helper</class>
            </sse_translationhints>
        </helpers>
        <models>
            <sse_translationhints>
                <class>SSE_TranslationHints_Model</class>
            </sse_translationhints>
            <core>
                <rewrite>
                    <translate>SSE_TranslationHints_Model_Translate</translate>
                </rewrite>
            </core>
        </models>
    </global>
</config>